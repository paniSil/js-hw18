{
  "name": "webpack-training",
  "version": "1.0.0",
  "description": "webpack init and training follow-up lection 1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=development webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "start": "cross-env NODE_ENV=development webpack serve --mode development --open",
    "analyze": "npx webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json dist/"
  },
